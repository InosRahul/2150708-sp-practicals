#include <string.h>
#include <stdio.h>
#include <conio.h>
void main()
{
   FILE *f1;
   char ch,str[300][30],str1[300][30],sym[300];
   int m=0,j=0,i=0,i1=0,j1=0,flag=0,p=0,q=0;
   clrscr();

   f1=fopen("117PARA.c","r");
   while((ch=getc(f1))!=EOF)
   {
	//printf("%c",ch);
	if(isalnum(ch) || ch=='_')
	{
		str[i][j++]=ch;
		flag=1;
	}
	else
	{
		if(!isspace(ch))
		{
			sym[m++]=ch;
		}
		if(flag==1)
		{
		       //	printf("%d\n",i);
			str[i][j]='\0';
			i++;
			j=0;
			flag=0;
		}


		if(ch=='"')
		{
			while((ch=getc(f1))!='"')
			{
				str1[i1][j1++]=ch;
			       //	printf("%d\n",i1);
			}
			 str1[i1][j1]='\0';
			 i1++;
			 j1=0;
		}
   }
   }
   fclose(f1);

//   printf("%d",i);
for(p=0;p<i;p++)
{
printf("%s\t",str[p]);
}
printf("\n---------------------------------\n");

for(p=0;p<m;p++)
{
printf("%c\t",sym[p]);
}
printf("\n---------------------------------\n");

for(p=0;p<i1;p++)
{
printf("%s\t",str1[p]);
}



